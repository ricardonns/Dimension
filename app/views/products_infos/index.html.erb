<h1>Listing products_infos</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Valuetype</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product.products_infos.each do |products_info| %>
  <tr>
    <td><%= products_info.description %></td>
    <td><%= products_info.valueType %></td>
    <td><%= products_info.value %></td>
    <td><%= link_to 'Edit', edit_product_products_info_path(@product, products_info) %></td>
    <td><%= link_to 'Destroy', product_products_info_path(@product, products_info), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Products info', new_product_products_info_path(@product) %>
